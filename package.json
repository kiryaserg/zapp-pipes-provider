    "axios": "^0.16.2"
  },
  "devDependencies": {
    "@applicaster/zapp-pipes-dev-kit": "^1.0.9",
    "ava": "^0.19.0",
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.3.2",
